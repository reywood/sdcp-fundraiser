{
    "root": true,
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "jquery": true
    },
    "rules": {
        "complexity": ["error", 8],
        "indent": ["error", 4],
        "max-len": ["error", 120],
        "prefer-const": 1,
        "quotes": ["error", "single"],
        "require-jsdoc": 0,
        "valid-jsdoc": 0
    }
}
